<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Darstellung</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.text>
                        <TCEforms>
                            <label>Text</label>
                            <config>
                                <type>text</type>
                                <enableRichtext>true</enableRichtext>
                                <cols>60</cols>
                                <rows>10</rows>
                            </config>
                        </TCEforms>
                    </settings.text>

                    <settings.pages.favorites>
                        <TCEforms>
                            <label>Favoriten Seite</label>
                            <config>
                                <type>select</type>
                                <renderType>selectTree</renderType>
                                <foreign_table>pages</foreign_table>
                                <size>5</size>
                                <maxitems>1</maxitems>
                                <treeConfig>
                                    <appearance>
                                        <expandAll>1</expandAll>
                                        <showHeader>1</showHeader>
                                    </appearance>
                                    <parentField>pid</parentField>
                                </treeConfig>
                            </config>
                        </TCEforms>
                    </settings.pages.favorites>
                    <settings.pages.bookings>
                        <TCEforms>
                            <label>Gebuchte Fahrten</label>
                            <config>
                                <type>select</type>
                                <renderType>selectTree</renderType>
                                <foreign_table>pages</foreign_table>
                                <size>5</size>
                                <maxitems>1</maxitems>
                                <treeConfig>
                                    <appearance>
                                        <expandAll>1</expandAll>
                                        <showHeader>1</showHeader>
                                    </appearance>
                                    <parentField>pid</parentField>
                                </treeConfig>
                            </config>
                        </TCEforms>
                    </settings.pages.bookings>
                    <settings.pages.account>
                        <TCEforms>
                            <label>Konto Einstellungen</label>
                            <config>
                                <type>select</type>
                                <renderType>selectTree</renderType>
                                <foreign_table>pages</foreign_table>
                                <size>5</size>
                                <maxitems>1</maxitems>
                                <treeConfig>
                                    <appearance>
                                        <expandAll>1</expandAll>
                                        <showHeader>1</showHeader>
                                    </appearance>
                                    <parentField>pid</parentField>
                                </treeConfig>
                            </config>
                        </TCEforms>
                    </settings.pages.account>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>